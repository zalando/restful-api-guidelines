extends: eslint:recommended
env:
  es6: true
rules:
  indent:
  - error
  - 2
  quotes:
  - 1
  - single
  - avoid-escape
  semi:
  - 2
  - always
  space-before-function-paren:
  - 2
  - always
  keyword-spacing:
  - 2
  - before: true
    after: true
  space-infix-ops: 2
  spaced-comment:
  - 2
  - always
  arrow-spacing: 2
  no-console: 2
  no-empty: 2
  eqeqeq:
  - 2
  - always
  no-unused-vars: 1
  no-unsafe-negation: 2
  prefer-const: 2
  no-var: 2
